<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Salary</th>
      <th>actions</th>
      <%# <th colspan="3"></th> %>
    </tr>
  </thead>

  <tbody>
    <% @proponents.each do |proponent| %>
      <tr>
        <td><%= proponent.name %></td>
        <td><%= proponent.salary %></td>

        <!--
        <td><%= link_to 'Show', proponent %></td>
        <td><%= link_to 'Edit', edit_proponent_path(proponent) %></td>
        -->

        <td>
          <%= button_to proponent_path(proponent), method: :delete, class: "btn btn-danger " do %>
            <i class="bi bi-trash"></i>
            Delete
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>


